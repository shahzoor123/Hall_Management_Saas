{
    "builds": [{
        "src": "morvin/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "morvin/wsgi.py"
        }
    ],
    "excludeFiles": [
        "*.chache",
        "static/**",
        "assets/**",
        "src/**"
    ]
}
